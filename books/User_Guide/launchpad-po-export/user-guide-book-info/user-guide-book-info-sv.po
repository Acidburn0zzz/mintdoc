msgid "User Guide"
msgstr "Användarguide"

#. Tag: subtitle
#, no-c-format
msgid "&EDITION; Edition"
msgstr "&UTGÅVA; Utgåva"

#. Tag: productname
#, no-c-format
msgid "&PRODUCT;"
